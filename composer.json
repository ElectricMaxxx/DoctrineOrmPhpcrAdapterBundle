{
    "name": "doctrine/orm-phpcr-adapter-bundle",
    "type": "library",
    "description": "Bundle to use the orm-odm-adapter library",
    "keywords": ["odm", "orm", "database"],
    "homepage": "http://www.doctrine-project.org",
    "license": "MIT",
    "authors":[
        {"name":"Maximilian Berghoff", "email":"maximilian.berghoff@gmx.de"}
    ],
    "repositories":[
        {"type":"vcs", "url":"https://github.com/ElectricMaxxx/DoctrineOrmOdmAdapter.git"}
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "doctrine/common": "~2.4.0",
        "doctrine/phpcr-odm": "1.1.*@dev",
        "doctrine/orm": "2.4.*@dev",
        "phpcr/phpcr": "2.1.1",
        "phpcr/phpcr-implementation": "~2.1.0",
        "phpcr/phpcr-utils": "~1.1.0",
        "jackalope/jackalope-doctrine-dbal": "~1.0",
        "doctrine/orm-odm-adapter":"1.0.*@dev"
    },
    "require-dev": {
        "symfony-cmf/testing": "dev-master",
        "matthiasnoback/symfony-dependency-injection-test": "0.*",
        "matthiasnoback/symfony-config-test": "0.*"
    },
    "suggest":{
        "symfony/yaml": "~2.0"
    },
    "autoload": {
        "psr-0": { "Doctrine\\ORM\\Bundle\\OrmPhpcrAdapterBundle": "" }
    },
    "target-dir": "Doctrine/ORM/Bundle/OrmPhpcrAdapterBundle",
    "bin": [],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
